{
  "name": "obs-twitch-gift",
  "version": "1.0.0",
  "description": "gift codes in twitch for obs studio",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev:ts": "ts-node src/index.ts",
    "build": "tsc && cp -R ./src/assets ./dist/assets",
    "build-babel": "babel src --extensions \".ts,.js\" --out-dir dist --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Veronesi/obs-twitch-gift.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Veronesi/obs-twitch-gift/issues"
  },
  "homepage": "https://github.com/Veronesi/obs-twitch-gift#readme",
  "dependencies": {
    "discord.js": "^14.10.2",
    "dotenv": "^16.0.3",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "inquirer": "^9.1.5",
    "obs-websocket-js": "^5.0.2",
    "tmi.js": "^1.8.5"
  },
  "devDependencies": {
    "@types/tmi.js": "^1.8.6",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-hexagonal-architecture": "^1.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^3.3.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
}